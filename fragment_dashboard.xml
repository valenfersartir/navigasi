<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".dashboard">


        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Welcome To My App"
            android:textSize="20dp"
            app:layout_constraintBottom_toTopOf="@+id/btnprofil"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btnprofil"
            android:layout_width="282dp"
            android:layout_height="54dp"
            android:layout_marginBottom="20dp"
            android:text="View My Profile"
            app:layout_constraintBottom_toTopOf="@+id/btnproject"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/btnproject"
            android:layout_width="282dp"
            android:layout_height="54dp"
            android:layout_marginBottom="24dp"
            android:text="View My Projects"
            app:layout_constraintBottom_toTopOf="@+id/btndiklat"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/btndiklat"
            android:layout_width="282dp"
            android:layout_height="54dp"
            android:layout_marginBottom="320dp"
            android:text="View My Certificates"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>